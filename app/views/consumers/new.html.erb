<h1>Consumers signup</h1>
<%= form_for :consumer, url: consumers_path do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br />

  <%= f.label :last_name %>
  <%= f.text_field :last_name %> <br />

  <%= f.label :email, "Email Address" %>
  <%= f.text_field :email  %> <br />

  <%= f.label :password %>
  <%= f.password_field :password %> <br />

 <br />

  <%= f.submit  %>

<% end %></p>
