
<h1>Edit details</h1>

<%= form_for :consumer, url: consumer_path ,method: "put" do |f| %>


  <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br />

  <%= f.label :last_name %>
  <%= f.text_field :last_name %> <br />

  <p>
    <%= f.submit %>
  </p>

<% end %>

<%= link_to 'Back', consumer_path %>
