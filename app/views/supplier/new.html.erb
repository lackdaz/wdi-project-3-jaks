<h1> Register a Business</h1>

<%= form_for @new_supplier do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :contact %>
  <%= f.text_field :contact %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password_digest %>
  <%= f.text_field :password_digest %>

  <%= f.label :website %>
  <%= f.text_field :website %>

  <%= f.submit%>

<% end %>
